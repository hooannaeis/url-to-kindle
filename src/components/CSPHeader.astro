---
const connectSrc =
  "connect-src 'self' https://sgtm.case-trace.com";
const scriptSrc = "script-src 'self' 'unsafe-inline' 'unsafe-eval'";
const objectSrc = "object-src 'none'";
const cspString = [connectSrc, scriptSrc, objectSrc].join("; ");
---

<meta http-equiv="Content-Security-Policy" content={cspString} />
